{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "nome",
    "cpf",
    "email",
    "valor",
    "parcelas",
    "seguro"
  ],
  "properties": {
    "nome": {
      "type": "string",
      "default": "",
      "title": "The nome Schema",
      "examples": [
        "Fulano de Tal"
      ]
    },
    "cpf": {
      "type": "integer",
      "default": 0,
      "title": "The cpf Schema",
      "examples": [
        97093236014
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "email@email.com"
      ]
    },
    "valor": {
      "type": "integer",
      "default": 0,
      "title": "The valor Schema",
      "examples": [
        1200
      ]
    },
    "parcelas": {
      "type": "integer",
      "default": 0,
      "title": "The parcelas Schema",
      "examples": [
        3
      ]
    },
    "seguro": {
      "type": "boolean",
      "default": false,
      "title": "The seguro Schema",
      "examples": [
        true
      ]
    }
  },
  "examples": [{
    "nome": "Fulano de Tal",
    "cpf": 97093236014,
    "email": "email@email.com",
    "valor": 1200,
    "parcelas": 3,
    "seguro": true
  }]
}
